    "flow-typed": "^2.5.1",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "lint-staged": "^7.2.0",
    "microbundle": "^0.6.0",
    "prettier": "^1.14.3",
    "prettylint": "^1.0.0",
    "react": "^16.5.2",
    "react-emotion": "^9.2.12",
    "rimraf": "^2.6.1",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-cleanup": "^3.0.0-beta.1",
    "rollup-plugin-commonjs": "^9.1.3",
