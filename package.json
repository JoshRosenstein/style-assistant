    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "bundlesize": "^0.17.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.2.0",
    "docz": "^0.11.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-tools": "^1.1.6",
